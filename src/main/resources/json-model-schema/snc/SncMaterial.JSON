{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "code": {
      "type": "integer"
    },
    "message": {
      "type": "string"
    },
    "data": {
      "type": "object",
      "properties": {
        "Material": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "DESCRIPTION": {
                  "type": "string"
                },
                "ITEM_NO": {
                  "type": "string"
                },
                "contains_BOM": {
                  "type": "object",
                  "properties": {
                    "PartNo": {
                      "type": "string"
                    },
                    "RequiredPartNo": {
                      "type": "string"
                    },
                    "RequiredQuantity": {
                      "type": "number"
                    },
                    "invert_Product": {
                      "type": "object",
                      "properties": {
                        "product_no": {
                          "type": "string"
                        },
                        "invert_Preactor_Order": {
                          "type": "array",
                          "items": [
                            {
                              "type": "object",
                              "properties": {
                                "product": {
                                  "type": "string"
                                },
                                "order_no": {
                                  "type": "string"
                                },
                                "invert_Product_Order_Process": {
                                  "type": "array",
                                  "items": [
                                    {
                                      "type": "object",
                                      "properties": {
                                        "mach_type": {
                                          "type": "string"
                                        },
                                        "pass_quantity": {
                                          "type": "string"
                                        },
                                        "produce_quantity": {
                                          "type": "string"
                                        },
                                        "product_order": {
                                          "type": "string"
                                        }
                                      },
                                      "required": [
                                        "mach_type",
                                        "pass_quantity",
                                        "produce_quantity",
                                        "product_order"
                                      ]
                                    }
                                  ]
                                }
                              },
                              "required": [
                                "product",
                                "order_no",
                                "invert_Product_Order_Process"
                              ]
                            }
                          ]
                        }
                      },
                      "required": [
                        "product_no",
                        "invert_Preactor_Order"
                      ]
                    }
                  },
                  "required": [
                    "PartNo",
                    "RequiredPartNo",
                    "RequiredQuantity",
                    "invert_Product"
                  ]
                }
              },
              "required": [
                "DESCRIPTION",
                "ITEM_NO",
                "contains_BOM"
              ]
            }
          ]
        }
      },
      "required": [
        "Material"
      ]
    }
  },
  "required": [
    "code",
    "message",
    "data"
  ]
}