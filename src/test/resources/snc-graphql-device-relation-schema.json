{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "code": {
      "type": "integer"
    },
    "message": {
      "type": "string"
    },
    "data": {
      "type": "object",
      "properties": {
        "Device": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string"
                },
                "Connect_To_Work_Position": {
                  "type": "object",
                  "properties": {
                    "invert_Work_Center": {
                      "type": "object",
                      "properties": {
                        "name": {
                          "type": "string"
                        },
                        "description": {
                          "type": "string"
                        },
                        "location": {
                          "type": "string"
                        },
                        "id": {
                          "type": "string"
                        },
                        "Has_Preactor_OrderConnection": {
                          "type": "object",
                          "properties": {
                            "totalElements": {
                              "type": "integer"
                            },
                            "totalPages": {
                              "type": "integer"
                            },
                            "pageSize": {
                              "type": "integer"
                            },
                            "page": {
                              "type": "integer"
                            },
                            "numberOfElements": {
                              "type": "integer"
                            },
                            "edges": {
                              "type": "array",
                              "items": [
                                {
                                  "type": "object",
                                  "properties": {
                                    "node": {
                                      "type": "object",
                                      "properties": {
                                        "order_no": {
                                          "type": "string"
                                        },
                                        "start_time": {
                                          "type": "string"
                                        },
                                        "product": {
                                          "type": "string"
                                        },
                                        "work_center": {
                                          "type": "string"
                                        },
                                        "comments": {
                                          "type": "null"
                                        },
                                        "end_time": {
                                          "type": "string"
                                        },
                                        "plan_quantity": {
                                          "type": "string"
                                        },
                                        "id": {
                                          "type": "null"
                                        },
                                        "Has_Product": {
                                          "type": "object",
                                          "properties": {
                                            "product_no": {
                                              "type": "string"
                                            },
                                            "name": {
                                              "type": "string"
                                            },
                                            "product_type": {
                                              "type": "string"
                                            }
                                          },
                                          "required": [
                                            "product_no",
                                            "name",
                                            "product_type"
                                          ]
                                        },
                                        "invert_Product_Order_Process": {
                                          "type": "array",
                                          "items": [
                                            {
                                              "type": "object",
                                              "properties": {
                                                "product": {
                                                  "type": "string"
                                                },
                                                "produce_quantity": {
                                                  "type": "string"
                                                },
                                                "pass_quantity": {
                                                  "type": "string"
                                                },
                                                "mach_type": {
                                                  "type": "string"
                                                },
                                                "product_order": {
                                                  "type": "string"
                                                },
                                                "event_time": {
                                                  "type": "string"
                                                },
                                                "status": {
                                                  "type": "string"
                                                }
                                              },
                                              "required": [
                                                "product",
                                                "produce_quantity",
                                                "pass_quantity",
                                                "mach_type",
                                                "product_order",
                                                "event_time",
                                                "status"
                                              ]
                                            }
                                          ]
                                        }
                                      },
                                      "required": [
                                        "order_no",
                                        "start_time",
                                        "product",
                                        "work_center",
                                        "comments",
                                        "end_time",
                                        "plan_quantity",
                                        "id",
                                        "Has_Product",
                                        "invert_Product_Order_Process"
                                      ]
                                    },
                                    "cursor": {
                                      "type": "string"
                                    }
                                  },
                                  "required": [
                                    "node",
                                    "cursor"
                                  ]
                                }
                              ]
                            },
                            "pageInfo": {
                              "type": "object",
                              "properties": {
                                "endCursor": {
                                  "type": "string"
                                },
                                "hasNextPage": {
                                  "type": "boolean"
                                }
                              },
                              "required": [
                                "endCursor",
                                "hasNextPage"
                              ]
                            }
                          },
                          "required": [
                            "totalElements",
                            "totalPages",
                            "pageSize",
                            "page",
                            "numberOfElements",
                            "edges",
