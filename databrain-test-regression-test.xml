        <parameter name="port" value="32084"/>
        <parameter name="baseUrlOfConnector" value="http://140.231.89.85"/>
        <parameter name="portOfConnector" value="30694"/>
      </class> <!-- com.siemens.datalayer.databrain.test.DataBrainFromApiEngineTests -->
    </classes>
  </test> <!-- Regression Tests for databrain-test -->
</suite> <!-- data-layer-automation test suite -->
